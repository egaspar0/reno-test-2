{
  "extends": [
    "config:best-practices"
  ],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "major": {
    "dependencyDashboardApproval": true,
    "addLabels": [
      "major-upgrade"
    ]
  },
  "vulnerabilityAlerts": {
    "groupName": "{{{vulnerabilitySeverity}}} vulnerable dependencies",
    "enabled": true,
    "labels": [
      "security"
    ]
  },
  "osvVulnerabilityAlerts": true
}
